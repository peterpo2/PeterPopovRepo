@model List<APTEKA_Software.Models.ViewModels.UserViewModel>

<!-- Тук добавете HTML код за таблица -->
<table class="table table-striped">
    <thead>
        <tr>
            <th>Потребителско име</th>
            <th>Име</th>
            <th>Фамилия</th>
            <th>Дата на регистрация</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Username</td>
                <td>@user.FirstName</td>
                <td>@user.LastName</td>
                <td>@user.DateRegistered.ToShortDateString()</td>
            </tr>
        }
    </tbody>
</table>
